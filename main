    <script>
        let money = 0;
        let spins = 0;
        let wins = 0;
        let totalWon = 0;
        let minesLeft = 3;
        let itemsMined = 0;
        let timerInterval = null;
        let timeRemaining = 0;
        let isLocked = false;
        
        const symbols = ['üçí', 'üçã', 'üçä', 'üçâ', '‚≠ê', 'üíé', '7Ô∏è‚É£'];
        const spinCost = 50;

        const miningItems = [
            { item: 'üíé', name: 'Diamond', value: 100, chance: 0.05 },
            { item: 'üí∞', name: 'Gold', value: 50, chance: 0.10 },
            { item: 'üü¶', name: 'Sapphire', value: 35, chance: 0.15 },
            { item: 'üü©', name: 'Emerald', value: 30, chance: 0.15 },
            { item: 'ü™ô', name: 'Coin', value: 20, chance: 0.20 },
            { item: 'ü™®', name: 'Rock', value: 5, chance: 0.35 }
        ];

        function mineBlock(index) {
            if (minesLeft <= 0 || isLocked) return;
            
            const blocks = document.querySelectorAll('.mine-block');
            if (blocks[index].classList.contains('mined')) return;
            
            blocks[index].classList.add('mined');
            minesLeft--;
            
            const random = Math.random();
            let cumulativeChance = 0;
            let foundItem = null;
            
            for (let item of miningItems) {
                cumulativeChance += item.chance;
                if (random <= cumulativeChance) {
                    foundItem = item;
                    break;
                }
            }
            
            const resultDiv = document.getElementById('miningResult');
            
            if (foundItem && random <= 0.65) {
                blocks[index].textContent = foundItem.item;
                money += foundItem.value;
                itemsMined++;
                resultDiv.textContent = `Found ${foundItem.item} ${foundItem.name}! +${foundItem.value}`;
                resultDiv.className = 'mining-result win';
            } else {
                blocks[index].textContent = '‚ùå';
                resultDiv.textContent = 'Nothing found...';
                resultDiv.className = 'mining-result lose';
            }
            
            updateDisplay();
        }

        function resetMining() {
            if (isLocked) return;
            
            minesLeft = 3;
            const blocks = document.querySelectorAll('.mine-block');
            blocks.forEach(block => {
                block.classList.remove('mined');
                block.textContent = '‚¨õ';
            });
            document.getElementById('miningResult').textContent = '';
            
            // Lock the mine
            isLocked = true;
            timeRemaining = 30;
            document.getElementById('miningWall').classList.add('active');
            document.getElementById('resetButton').disabled = true;
            document.getElementById('resetButton').style.opacity = '0.5';
            
            // Start countdown
            updateTimer();
            timerInterval = setInterval(() => {
                timeRemaining--;
                updateTimer();
                
                if (timeRemaining <= 0) {
                    clearInterval(timerInterval);
                    unlockMine();
                }
            }, 1000);
            
            updateDisplay();
        }

        function updateTimer() {
            document.getElementById('timerDisplay').textContent = timeRemaining;
            
            // Shake locks when timer gets low
            if (timeRemaining <= 5 && timeRemaining > 0) {
                document.getElementById('lock1').classList.add('shake');
                document.getElementById('lock2').classList.add('shake');
                setTimeout(() => {
                    document.getElementById('lock1').classList.remove('shake');
                    document.getElementById('lock2').classList.remove('shake');
                }, 500);
            }
        }

        function unlockMine() {
            const wall = document.getElementById('miningWall');
            wall.classList.add('breaking');
            
            // Break the locks
            document.getElementById('lock1').style.animation = 'fallDown 0.8s ease-in forwards';
            document.getElementById('lock2').style.animation = 'fallDown 0.8s ease-in forwards';
            
            // Break all planks
            const planks = document.querySelectorAll('.plank');
            planks.forEach((plank, index) => {
                setTimeout(() => {
                    plank.classList.add('falling');
                }, index * 50);
            });
            
            // Hide wall after animation
            setTimeout(() => {
                isLocked = false;
                wall.classList.remove('active', 'breaking');
                document.getElementById('resetButton').disabled = false;
                document.getElementById('resetButton').style.opacity = '1';
                
                // Reset planks for next time
                planks.forEach(plank => {
                    plank.classList.remove('falling');
                    plank.style.animation = '';
                });
                document.getElementById('lock1').style.animation = '';
                document.getElementById('lock2').style.animation = '';
            }, 1000);
        }

        function pullLever() {
            if (money < spinCost) {
                document.getElementById('message').textContent = '‚ùå Not enough money!';
                document.getElementById('message').className = 'message lose';
                return;
            }

            money -= spinCost;
            spins++;
            
            const pullButton = document.getElementById('pullButton');
            pullButton.disabled = true;
            document.getElementById('message').textContent = '';

            const reels = [
                document.getElementById('reel1'),
                document.getElementById('reel2'),
                document.getElementById('reel3')
            ];

            reels.forEach(reel => reel.classList.add('spinning'));

            let spinInterval = setInterval(() => {
                reels.forEach(reel => {
                    reel.textContent = symbols[Math.floor(Math.random() * symbols.length)];
                });
            }, 100);

            setTimeout(() => {
                clearInterval(spinInterval);
                
                const results = [
                    symbols[Math.floor(Math.random() * symbols.length)],
                    symbols[Math.floor(Math.random() * symbols.length)],
                    symbols[Math.floor(Math.random() * symbols.length)]
                ];

                reels.forEach((reel, i) => {
                    reel.textContent = results[i];
                    reel.classList.remove('spinning');
                });

                checkWin(results);
                pullButton.disabled = false;
                updateDisplay();
            }, 2000);
        }

        function checkWin(results) {
            const msg = document.getElementById('message');
            
            if (results[0] === results[1] && results[1] === results[2]) {
                let winAmount = 0;
                
                if (results[0] === 'üíé') {
                    winAmount = 1000;
                    msg.textContent = 'üíé JACKPOT! Won $1000! üíé';
                } else if (results[0] === '7Ô∏è‚É£') {
                    winAmount = 500;
                    msg.textContent = 'üéâ TRIPLE 7s! Won $500! üéâ';
                } else if (results[0] === '‚≠ê') {
                    winAmount = 300;
                    msg.textContent = '‚≠ê AMAZING! Won $300! ‚≠ê';
                } else {
                    winAmount = 150;
                    msg.textContent = `üéä Three ${results[0]}! Won $150! üéä`;
                }
                
                money += winAmount;
                wins++;
                totalWon += winAmount;
                msg.className = 'message win';
            } else if (results[0] === results[1] || results[1] === results[2] || results[0] === results[2]) {
                money += 25;
                totalWon += 25;
                msg.textContent = 'üòä Two match! Won $25!';
                msg.className = 'message win';
            } else {
                msg.textContent = 'üòî No match. Try again!';
                msg.className = 'message lose';
            }
        }

        function updateDisplay() {
            document.getElementById('money').textContent = money;
            document.getElementById('spins').textContent = spins;
            document.getElementById('wins').textContent = wins;
            document.getElementById('totalWon').textContent = totalWon;
            document.getElementById('minesLeft').textContent = minesLeft;
            document.getElementById('itemsMined').textContent = itemsMined;
        }

        updateDisplay();
    </script>
</body>
</html>

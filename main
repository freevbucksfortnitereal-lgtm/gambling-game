    <div class="game-container">
        <h1>üé∞ Slot Machine üé∞</h1>
        
        <div class="money-display">
            üí∞ Money: $<span id="money">0</span>
        </div>

        <div class="mining-section">
            <div class="mining-wall" id="miningWall">
                <div class="planks-container" id="planksContainer">
                    <div class="plank plank-horizontal" style="top: 10%;"></div>
                    <div class="plank plank-horizontal" style="top: 30%;"></div>
                    <div class="plank plank-horizontal" style="top: 50%;"></div>
                    <div class="plank plank-horizontal" style="top: 70%;"></div>
                    <div class="plank plank-horizontal" style="bottom: 10%;"></div>
                    <div class="plank plank-vertical" style="left: 15%;"></div>
                    <div class="plank plank-vertical" style="left: 45%;"></div>
                    <div class="plank plank-vertical" style="right: 15%;"></div>
                </div>
                <div class="lock" id="lock1" style="top: 35%; left: 50%; transform: translateX(-50%);">üîí</div>
                <div class="lock" id="lock2" style="top: 55%; left: 50%; transform: translateX(-50%);">üîí</div>
                <div class="wall-content">
                    <div class="wall-text">üîí Mine Resetting...</div>
                    <div class="timer-display" id="timerDisplay">30</div>
                </div>
            </div>
            
            <div class="mining-title">‚õèÔ∏è Mining Zone ‚õèÔ∏è</div>
            <div class="mines-left">Mines Left: <span id="minesLeft">3</span>/3</div>
            
            <div class="mining-grid" id="miningGrid">
                <div class="mine-block" onclick="mineBlock(0)">‚¨õ</div>
                <div class="mine-block" onclick="mineBlock(1)">‚¨õ</div>
                <div class="mine-block" onclick="mineBlock(2)">‚¨õ</div>
                <div class="mine-block" onclick="mineBlock(3)">‚¨õ</div>
                <div class="mine-block" onclick="mineBlock(4)">‚¨õ</div>
                <div class="mine-block" onclick="mineBlock(5)">‚¨õ</div>
                <div class="mine-block" onclick="mineBlock(6)">‚¨õ</div>
                <div class="mine-block" onclick="mineBlock(7)">‚¨õ</div>
                <div class="mine-block" onclick="mineBlock(8)">‚¨õ</div>
            </div>

            <div class="mining-result" id="miningResult"></div>
            <button class="reset-mines" id="resetButton" onclick="resetMining()">üîÑ Reset Mining (Free)</button>
